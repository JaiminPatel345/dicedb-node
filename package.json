{
  "name": "dicedb-node",
  "version": "1.0.0",
  "description": "Node.js client sdk of diceDB",
  "type": "module",
  "main": "src/index.ts",
  "repository": "https://github.com/JaiminPatel345/dicedb-node.git",
  "author": "Jaimin Detroja",
  "keywords": ["dicedb" , "dicedb-node" ,  "node-dicedb" , "node dicedb"],
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepublishOnly": "yarn build"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "ts-jest": "^29.2.6"
  }

}
